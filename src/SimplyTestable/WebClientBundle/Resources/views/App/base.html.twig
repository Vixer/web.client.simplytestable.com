<!DOCTYPE html>
<html>
    <head>
        <meta charset="UTF-8" />
        <title>{% block title %}Simply Testable{% endblock %}</title>
        {% block stylesheets %}
        <link rel="stylesheet" href="/bundles/simplytestablewebclient/vendor/twitter-bootstrap/bootstrap/css/bootstrap.min.css" />        
        <link rel="stylesheet" href="/bundles/simplytestablewebclient/vendor/font-awesome/latest/css/font-awesome.css" />
        <link rel="stylesheet" href="/bundles/simplytestablewebclient/css/main.css" />        
        <link rel="stylesheet" href="/bundles/simplytestablewebclient/css/app.css" />
        {% endblock %}
        <link rel="icon" type="image/x-icon" href="{{ asset('favicon.ico') }}" />
    </head>
    <body  class="{% block body_class %}{{ app.request.get('_route') | replace({'_' : '-'}) }}{% endblock %}">
        {% block body %}{% endblock %}
        {% block javascripts %}
            <script type="text/javascript" src="https://ajax.googleapis.com/ajax/libs/jquery/1.7.2/jquery.min.js"></script>
            <script type="text/javascript" src="/bundles/simplytestablewebclient/vendor/twitter-bootstrap/bootstrap/js/bootstrap.min.js"></script>
            <script type="text/javascript" src="/bundles/simplytestablewebclient/js/app.js"></script>
        {% endblock %}

        {% include 'SimplyTestableWebClientBundle:Partials:navbar.html.twig' %}                                
            
        <div class="outer">
            <div id="main">
                {% block main %}{% endblock %} 
            </div>                       
        </div>
    </body>
</html>
